{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-param-reassign": "off",
        "default-case": "off",
        "prefer-rest-params": "off",
        "no-restricted-syntax": "off",
        "no-mixed-operators": "off",
        "no-plusplus": "off"
    }
}